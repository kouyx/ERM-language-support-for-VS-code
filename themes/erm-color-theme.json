{
  "name": "ERM Dark Theme",
  "type": "dark",
  "colors": {
    "editor.foreground": "#D4D4D4",
    "editor.background": "#1E1E1E",
    "editor.lineHighlightBackground": "#2A2A2A",
    "editor.lineHighlightBorder": "#2A2A2A",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#C6C6C6",
    "editor.selectionBackground": "#264F78",
    "editorCursor.foreground": "#AEAFAD",
    "editorWhitespace.foreground": "#404040",
    "editorIndentGuide.background1": "#404040",
    "editorIndentGuide.activeBackground1": "#707070",
    "editorBracketMatch.background": "#006400",
    "editorBracketMatch.border": "#006400"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment.line.erm",
        "comment.inline.erm"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Todo/Fixme",
      "scope": "keyword.other.todo.erm",
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Header ZVSE",
      "scope": "keyword.control.header.erm",
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Receiver !!",
      "scope": "keyword.operator.receiver.erm",
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Receiver Name",
      "scope": "entity.name.function.receiver.erm",
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Instruction !#",
      "scope": "keyword.operator.instruction.erm",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Instruction Name",
      "scope": "entity.name.function.instruction.erm",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Trigger !?",
      "scope": "keyword.operator.trigger.erm",
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Trigger ID",
      "scope": "entity.name.type.trigger.erm",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "decorator.function_name.erm",
        "entity.name.function.custom.erm"
      ],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Commands",
      "scope": [
        "command.erm",
        "keyword.control.command.erm"
      ],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables/Registers",
      "scope": [
        "variable.erm",
        "variable.parameter.register.erm"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric.erm",
        "constant.numeric.integer.erm",
        "constant.numeric.trigger-id.erm"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Named Constants",
      "scope": [
        "constant.named.erm",
        "constant.other.named-constant.erm"
      ],
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Labels in Brackets",
      "scope": [
        "variable.label.erm",
        "variable.other.label.erm"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "name": "Macro Declaration",
      "scope": [
        "string.macro_declaration.erm",
        "entity.name.tag.macro-declaration.erm"
      ],
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Macro Usage",
      "scope": [
        "string.macro.erm",
        "entity.name.tag.macro-usage.erm"
      ],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Caret Strings",
      "scope": [
        "string.caret.erm",
        "string.quoted.caret.erm"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "keyword.operator.interpolation.erm",
        "constant.character.escape.interpolation.erm"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Double Quoted Strings",
      "scope": "string.quoted.double.erm",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape.erm",
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Boolean Constants",
      "scope": "constant.language.boolean.erm",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Symbol Constants",
      "scope": "constant.other.symbol.erm",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Comparison Operators",
      "scope": "keyword.operator.comparison.erm",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Arithmetic Operators",
      "scope": "keyword.operator.arithmetic.erm",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Assignment Operator",
      "scope": "keyword.operator.assignment.erm",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Logical Operators",
      "scope": "keyword.operator.logical.erm",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Separators",
      "scope": "punctuation.separator.erm",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Parentheses",
      "scope": [
        "punctuation.parenthesis.open.erm",
        "punctuation.parenthesis.close.erm"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Whitespace",
      "scope": "punctuation.whitespace.erm",
      "settings": {}
    }
  ]
}