{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["^", "^"]
  ],
  "autoClosingPairs": [
    {"open": "{", "close": "}"},
    {"open": "[", "close": "]"},
    {"open": "(", "close": ")"},
    {"open": "^", "close": "^"},
    {"open": "%(", "close": ")"}
  ],
  "autoCloseBefore": ";:.,=}])` \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["^", "^"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*//\\s*#region\\b",
      "end": "^\\s*//\\s*#endregion\\b"
    }
  },
  "wordPattern": "[a-zA-Z][a-zA-Z0-9_]*|[xyvzwefc]-?\\d+|[g-t]|\\$(?!\\$)|@(?!@)|%[a-zA-Z]",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*!![A-Z]{2}:.*$",
    "decreaseIndentPattern": "^\\s*!![a-z]{2};"
  }
}